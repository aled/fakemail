server {
    listen 80;
    return 301 https://$host$request_uri;
}

server {
    listen 443;
    server_name www.fakemail.stream;

    location / {
        proxy_pass http://localhost:8080;
        proxy_read_timeout 90;                
    }

    proxy_redirect http://localhost:8080 https://www.fakemail.stream
}
