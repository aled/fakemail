{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Api": {
    "BaseUri": "http://localhost:5053/api/"
  },
  "IpRateLimit": {
    "CacheSize": 1000,
    "RateLimitDefinitions": [
      {
        "MaxRequests": 20,
        "Period": "00:00:01"
      },
      {
        "MaxRequests": 60,
        "Period": "00:01:00"
      },
      {
        "MaxRequests": 180,
        "Period": "00:15:00"
      }
    ]
  }
}
